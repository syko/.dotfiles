[user]
  name = 
  email = 
[alias]
  co = checkout
  ci = commit
  st = status
  ba = branch -a
  pull = pull --rebase
[color]
  diff = auto
  status = auto
  branch = auto
  sh = auto
[branch]
  autosetuprebase = always
[mergetool]
  keepBackup = false
  keepTemporaries = false
  prompt = false
[giggle]
  main-window-maximized = false
  main-window-geometry = 1283x898+388+83
  history-view-vpane-position = 446
  main-window-view = HistoryView
  file-view-vpane-position = 357
[push]
  default = current
[core]
  editor = vim
  quotepath = false
  excludesfile = /home/syko/.dotfiles/linux_extra/.gitignore_global
  ignorecase = false
