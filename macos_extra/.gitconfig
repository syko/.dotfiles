[user]
  name =
  email =
[merge]
  tool = diffmerge
[alias]
  co = checkout
  ci = commit
  st = status
  ba = branch -a
  pull = pull --rebase
[color]
  diff = auto
  status = auto
  branch = auto
  sh = auto
[branch]
  autosetuprebase = always
[mergetool]
  keepBackup = false
  keepTemporaries = false
  prompt = false
[giggle]
  main-window-maximized = false
  main-window-geometry = 1283x898+388+83
  history-view-vpane-position = 446
  main-window-view = HistoryView
  file-view-vpane-position = 357
[push]
  default = current
[core]
  editor = vim
  quotepath = false
  excludesfile = /Users/andrin/.gitignore_global
  ignorecase = false
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path = 
[mergetool "sourcetree"]
  cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[diff]
  tool = diffmerge
[difftool "diffmerge"]
  cmd = /usr/local/bin/diffmerge \"$LOCAL\" \"$REMOTE\"
[mergetool "diffmerge"]
  trustExitCode = true
  cmd = /usr/local/bin/diffmerge --merge --result=\"$MERGED\" \"$LOCAL\" \"$BASE\" \"$REMOTE\"
